{
  "name": "GASocialLogin",
  "version": "2.6.4",
  "summary": "Easy social media log in",
  "description": "Easy social media log in.",
  "homepage": "https://github.com/Gini-Apps/GASocialLogin",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "idoMeirov": "idom@gini-apps.com"
  },
  "source": {
    "git": "https://github.com/Gini-Apps/GASocialLogin.git",
    "tag": "2.6.4"
  },
  "swift_versions": "5.0",
  "platforms": {
    "ios": "9.0"
  },
  "frameworks": "UIKit",
  "default_subspecs": "Core",
  "subspecs": [
    {
      "name": "Core",
      "source_files": "GASocialLogin/Core/**/*"
    },
    {
      "name": "Facebook",
      "source_files": "GASocialLogin/Facebook/**/*",
      "dependencies": {
        "GASocialLogin/Core": [

        ],
        "FBSDKLoginKit": [
          "~> 7.1.1"
        ]
      }
    },
    {
      "name": "Google",
      "source_files": "GASocialLogin/Google/**/*",
      "dependencies": {
        "GASocialLogin/Core": [

        ],
        "GoogleSignIn": [
          "~> 6.0.1"
        ]
      }
    },
    {
      "name": "Apple",
      "source_files": "GASocialLogin/Apple/**/*",
      "frameworks": "AuthenticationServices",
      "dependencies": {
        "GASocialLogin/Core": [

        ]
      }
    }
  ],
  "swift_version": "5.0"
}
